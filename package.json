{
  "name": "rocket-league-workshop",
  "version": "0.1.0",
  "private": true,
  "main": "public/run-electron.js",
  "scripts": {
    "start": "npm-run-all --parallel start:react start:electron",
    "start:electron": "electron .",
    "start:react": "webpack serve --config ./webpack.config.js --mode development",
    "bundle:react": "webpack --mode production",
    "dist": "yarn run \"bundle:react\" && node installers/windows/installer.js"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "electron": "^12.0.7",
    "electron-devtools-installer": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
